{
  "productName": "pts_bomber",
  "version": "2.0.0",
  "identifier": "ru.pts-team.pts-bomber",
  "build": {
    "beforeDevCommand": "trunk serve",
    "devUrl": "http://localhost:3229",
    "beforeBuildCommand": "trunk build --release",
    "frontendDist": "../dist"
  },
  "app": {
    "withGlobalTauri": true,
    "windows": [
      {
        "title": "pts_bomber",
        "width": 800,
        "height": 600
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}
